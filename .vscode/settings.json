  "mikuPlayer.commands.playPause": "qdbus org.mpris.MediaPlayer2.plasma-browser-integration /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.PlayPause",
  "mikuPlayer.commands.next": "qdbus org.mpris.MediaPlayer2.plasma-browser-integration /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Next",
  "mikuPlayer.commands.previous": "qdbus org.mpris.MediaPlayer2.plasma-browser-integration /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Previous",
  "mikuPlayer.commands.volumeUp": "pactl set-sink-volume @DEFAULT_SINK@ +5%",
  "mikuPlayer.commands.volumeDown": "pactl set-sink-volume @DEFAULT_SINK@ -5%",
  "mikuPlayer.commands.mute": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
  "mikuPlayer.metadata.enabled": true,
  "mikuPlayer.metadata.artistCommand": "qdbus org.mpris.MediaPlayer2.plasma-browser-integration /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Metadata | awk -F': ' '/xesam:artist/ {print $2}'",
  "mikuPlayer.metadata.titleCommand": "qdbus org.mpris.MediaPlayer2.plasma-browser-integration /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Metadata | awk -F': ' '/xesam:title/ {print $2}'",
  "mikuPlayer.backgroundImageCommand": "qdbus org.mpris.MediaPlayer2.plasma-browser-integration /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Metadata | awk -F': ' '/mpris:artUrl/ {print $2}'",
  "mikuPlayer.backgroundEnabled": true,
  "mikuPlayer.buttonLabels": {
    "play-pause": "‚ñ∂Play/Pause",
    "previous": "‚èÆ Back",
    "next": "‚è≠ Skip",
    "volume-down": "üîâ Lower",
    "volume-up": "üîä Raise",
    "mute": "üîá Mute"
  },
